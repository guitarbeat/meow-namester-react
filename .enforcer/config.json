{
  "debug_mode_enabled": false,
  "check_fixtures": false,
  "check_submodules": false,
  "disabled_rules": {
    "javascript": [],
    "typescript": []
  },
  "custom_fixture_patterns": {
    "javascript": [
      "**/test/**",
      "**/tests/**",
      "**/__tests__/**",
      "**/*.test.js",
      "**/*.test.jsx",
      "**/*.spec.js",
      "**/*.spec.jsx"
    ],
    "typescript": [
      "**/test/**",
      "**/tests/**",
      "**/__tests__/**",
      "**/*.test.ts",
      "**/*.test.tsx",
      "**/*.spec.ts",
      "**/*.spec.tsx"
    ]
  }
}
