{
  "name": "meow-namester",
  "version": "1.0.1",
  "description": "A React application for managing cat names and related data",
  "main": "index.js",
  "scripts": {
    "start": "craco start",
    "dev": "craco start",
    "build": "GENERATE_SOURCEMAP=false craco build",
    "build:analyze": "GENERATE_SOURCEMAP=false ANALYZE=true craco build",
    "test": "craco test",
    "eject": "react-scripts eject",
    "lint:css": "stylelint \"src/**/*.css\" --fix",
    "lint:js": "eslint \"src/**/*.{js,jsx}\" --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,css,scss}\"",
    "lint": "npm run lint:js && npm run lint:css",
    "prepare": "husky install",
    "audit:fix": "npm audit fix"
  },
  "keywords": [
    "react",
    "cats",
    "names",
    "management"
  ],
  "author": "Aaron",
  "license": "UNLICENSED",
  "dependencies": {
    "@hello-pangea/dnd": "^18.0.1",
    "@heroicons/react": "^2.2.0",
    "@supabase/supabase-js": "^2.49.3",
    "ajv": "^8.17.1",
    "ajv-keywords": "^5.1.0",
    "chart.js": "^4.4.8",
    "node-fetch": "^3.3.2",
    "postcss": "^8.4.31",
    "react": "^19.0.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.0.0",
    "react-scripts": "^5.0.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-transform-private-property-in-object": "^7.21.11",
    "@craco/craco": "^7.1.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "postcss-preset-env": "^10.1.5",
    "prettier": "^3.5.3",
    "stylelint": "^16.23.1",
    "stylelint-config-recess-order": "^6.1.0",
    "stylelint-config-standard": "^37.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.8.0",
    "stylelint-declaration-strict-value": "^1.10.0",
    "webpack-bundle-analyzer": "^4.10.2"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,scss}": [
      "stylelint --fix",
      "prettier --write"
    ]
  }
}
